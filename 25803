#define _CRT_SECURE_NO_WARNINGS
#include <stdio.h>

int main() {
    /*
         ,r'"7
r`-_   ,'  ,/
 \. ". L_r'
   `~\/
      |
      |
      */

	printf("         ,r'\"7\n");
	printf("r`-_   ,'  ,/\n");
	printf(" \\. \". L_r'\n");
	printf("   `~\\/\n");
	printf("      |\n");
	printf("      |\n");
	return 0;
}
